{% extends 'base.html.twig' %}
{% block title %}Actualizar - Profesores{% endblock %}
{% block container %}
    <nav aria-label="breadcrumb">
        <ol class="breadcrumb mb-4">
            <li class="breadcrumb-item"><a href="{{ path('incio') }}">Incio</a></li>
            <li class="breadcrumb-item"><a href="{{ path('profesores') }}">Profesores</a></li>
            <li class="breadcrumb-item active" aria-current="page">Actualizar</li>
        </ol>
    </nav>
    <div class="row">
        <div class="col-md-12">
            {{ form_start(form, { attr: {novalidate: 'novalidate'}}) }}
            {{ form_errors(form) }}
                {{ form_row(form.nif) }}
                {{ form_row(form.nombre) }}
                {{ form_row(form.apellido1) }}
                {{ form_row(form.apellido2) }}
                {{ form_row(form.fichero) }}
                {{ form_row(form.username) }}
                {{ form_row(form.email) }}
                {{ form_row(form.plainPassword) }}
            {{ form_end(form) }}
        </div>
    </div>
{% endblock %}